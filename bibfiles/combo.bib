keywords = {Chemistry, Physical,Chemistry, Physical: methods,Computer Simulation,Diffusion,Entropy,Hardness,Kinetics,Models, Chemical,Models, Statistical,Models, Theoretical,Molecular Conformation,Monte Carlo Method,Thermodynamics},
number = {24},
pages = {244708},
pmid = {17614578},
title = {{Does confining the hard-sphere fluid between hard walls change its average properties?}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17614578},
volume = {126},
year = {2007}
}
@article{Krekelberg2008b,
abstract = {The residual multiparticle entropy (RMPE) of two- and three-dimensional fluids changes sign near the freezing line, providing a quasiuniversal "one-phase" rule for the location of the liquid-solid transition. We present new simulation results for d-dimensional hard-sphere fluids (d=1-5) which show, however, that this freezing criterion fails in other spatial dimensions. The results also call into question the idea that a change in sign of the RMPE implies the emergence of a new kind of local structural order in the fluid.},
author = {Krekelberg, William P and Shen, Vincent K and Errington, Jeffrey R and Truskett, Thomas M},
