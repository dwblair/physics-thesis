journal = {Nature},
keywords = {Animals,Body Size,Body Size: genetics,Body Weight,Body Weight: genetics,Crosses, Genetic,Evolution,Female,Male,Mice,Mice, Inbred Strains,Models, Genetic,Mutation,Mutation: genetics,Phenotype,Quantitative Trait Loci,Quantitative Trait Loci: genetics,Selection (Genetics),Skeleton},
number = {7186},
pages = {470--2},
pmid = {18368117},
shorttitle = {Nature},
title = {{Pleiotropic scaling of gene effects and the 'cost of complexity'.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18368117},
volume = {452},
year = {2008}
}
