keywords = {Algorithms,Artifacts,Chemotaxis,Chemotaxis: physiology,Computer Simulation,Eukaryotic Cells,Eukaryotic Cells: cytology,Eukaryotic Cells: physiology,Models, Biological,Monte Carlo Method,Stochastic Processes},
month = dec,
