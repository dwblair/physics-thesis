pages = {1--12},
title = {{Dual Geometric Worm Algorithm for Two-Dimensional Discrete Classical  Lattice Models}},
url = {http://link.aps.org/doi/10.1103/PhysRevE.70.016702},
volume = {70},
year = {2003}
}
