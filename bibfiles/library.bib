}
@article{Paul2001a,
abstract = {We develop a method of constructing percolation clusters that allows us to
build very large clusters using very little computer memory by limiting the
maximum number of sites for which we maintain state information to a number of
the order of the number of sites in the largest chemical shell of the cluster
being created. The memory required to grow a cluster of mass s is of the order
of \$s\^{}\backslash theta\$ bytes where \$\backslash theta\$ ranges from 0.4 for 2-dimensional lattices
to 0.5 for 6- (or higher)-dimensional lattices. We use this method to estimate
\$d\_\{\backslash scriptsize min\}\$, the exponent relating the minimum path \$\backslash ell\$ to the
Euclidean distance r, for 4D and 5D hypercubic lattices. Analyzing both site
and bond percolation, we find \$d\_\{\backslash scriptsize min\}=1.607\backslash pm 0.005\$ (4D) and
\$d\_\{\backslash scriptsize min\}=1.812\backslash pm 0.006\$ (5D). In order to determine
\$d\_\{\backslash scriptsize min\}\$ to high precision, and without bias, it was necessary to
first find precise values for the percolation threshold, \$p\_c\$:
\$p\_c=0.196889\backslash pm 0.000003\$ (4D) and \$p\_c=0.14081\backslash pm 0.00001\$ (5D) for site and
\$p\_c=0.160130\backslash pm 0.000003\$ (4D) and \$p\_c=0.118174\backslash pm 0.000004\$ (5D) for bond
percolation. We also calculate the Fisher exponent, \$\backslash tau\$, determined in the
course of calculating the values of \$p\_c\$: \$\backslash tau=2.313\backslash pm 0.003\$ (4D) and
\$\backslash tau=2.412\backslash pm 0.004\$ (5D).},
arxivId = {cond-mat/0101136v1},
author = {Paul, Gerald and Ziff, Robert M. and Stanley, and H. Eugene},
doi = {10.1103/PhysRevE.64.026115},
file = {:home/dwblair/Dropbox/dwbdocs/readings\_articles/0101136v1.pdf:pdf},
issn = {1063-651X},
journal = {Physical Review E},
keywords = {Disordered Systems and Neural Networks,Statistical Mechanics},
month = jul,
number = {2},
title = {{Percolation threshold, Fisher exponent, and shortest path exponent for four and five dimensions}},
url = {http://link.aps.org/doi/10.1103/PhysRevE.64.026115},
volume = {64},
year = {2001}
}
@article{Krauth1994,
abstract = {In this paper we present a dynamical Monte Carlo algorithm which is
applicable to systems satisfying a clustering condition: during the dynamical
evolution the system is mostly trapped in deep local minima (as happens in
glasses, pinning problems etc.). We compare the algorithm to the usual Monte
Carlo algorithm, using as an example the Bernasconi model. In this model, a
straightforward implementation of the algorithm gives an improvement of several
orders of magnitude in computational speed with respect to a recent, already
very efficient, implementation of the algorithm of Bortz, Kalos and Lebowitz.},
