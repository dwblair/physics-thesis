keywords = {Adenosine Triphosphate,Adenosine Triphosphate: chemistry,Adenosine Triphosphate: metabolism,Computer Simulation,Energy Transfer,Energy Transfer: physiology,Kinetics,Likelihood Functions,Models, Biological,Models, Chemical,Models, Statistical,Molecular Motor Proteins,Molecular Motor Proteins: chemistry,Molecular Motor Proteins: physiology,Motion,Movement,Movement: physiology,Myosin Type V,Myosin Type V: chemistry,Myosin Type V: metabolism,Proton-Translocating ATPases,Proton-Translocating ATPases: chemistry,Proton-Translocating ATPases: metabolism},
month = aug,
number = {4},
pages = {1156--68},
