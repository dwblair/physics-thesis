file = {:home/dwblair/Dropbox/dwbdocs/readings\_articles/ASKim008\_Review.pdf:pdf},
issn = {0001-8686},
journal = {Advances in colloid and interface science},
keywords = {brownian dynamics,diffusivity,equation,inter-particle interaction,langevin,metropolis method,molecular dynamics,monte carlo,stochastic force,verlet algorithm},
number = {1-3},
pages = {159--73},
pmid = {15581559},
title = {{Brownian Dynamics, Molecular Dynamics, and Monte Carlo modeling of colloidal systems.}},
